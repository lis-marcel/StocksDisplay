<Window x:Class="StocksDisplay.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Stocks Display" Height="350" Width="525">
    <Grid>
        <StackPanel>
            <Button Content="Fetch stocks of Democracy" Click="Button_Click" />
            <TextBlock x:Name="SymbolTextBlock" />
            <TextBlock x:Name="NewestTextBlock" />
            <TextBlock x:Name="GrowthTextBlock" />
        </StackPanel>
    </Grid>
</Window>
